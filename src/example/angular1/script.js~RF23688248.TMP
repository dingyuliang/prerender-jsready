    var module = angular.module("prerenderExample",[]);

    module.controller("prerenderExampleController", ["$http", function ($http) {
       
        $http({
            url: "../data.json?t=" + (new Date()),
            method: "GET",
            headers: { 'Content-Type': 'application/json; charset=UTF-8' },
        })
        .then({
            success: function (data) {
                console.log(12)
                angular.element(".ajax-data-container").append("<div> Success: " + " " + new Date() + data + "</div>");
            },
            error: function (response) {
                console.log(13)
                angular.element(".ajax-data-container").append("<div> Error: " + " " + new Date() + response + "</div>");
            }
        });


